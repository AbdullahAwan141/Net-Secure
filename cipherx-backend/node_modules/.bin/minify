#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspaces/Net-Secure/cipherx-backend/node_modules/.pnpm/minify@14.0.0/node_modules/minify/bin/node_modules:/workspaces/Net-Secure/cipherx-backend/node_modules/.pnpm/minify@14.0.0/node_modules/minify/node_modules:/workspaces/Net-Secure/cipherx-backend/node_modules/.pnpm/minify@14.0.0/node_modules:/workspaces/Net-Secure/cipherx-backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspaces/Net-Secure/cipherx-backend/node_modules/.pnpm/minify@14.0.0/node_modules/minify/bin/node_modules:/workspaces/Net-Secure/cipherx-backend/node_modules/.pnpm/minify@14.0.0/node_modules/minify/node_modules:/workspaces/Net-Secure/cipherx-backend/node_modules/.pnpm/minify@14.0.0/node_modules:/workspaces/Net-Secure/cipherx-backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/minify@14.0.0/node_modules/minify/bin/minify.js" "$@"
else
  exec node  "$basedir/../.pnpm/minify@14.0.0/node_modules/minify/bin/minify.js" "$@"
fi
